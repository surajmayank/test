<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="MaintenanceList" table="maintenancelist">
    <id name="maintenanceId" column="MAINTENANCE_ID"> <generator class="native"/>  </id>
   <property name="maintenanceName" column="MAINTENANCE_NAME" type="java.lang.String"></property>
   <property name="maintenanceCharge" column="MAINTENANCE_CHARGE" type="integer"/>
  </class>
</hibernate-mapping>